      body {
        font-family: "Roboto", serif;
        margin: 0;
        background-color: #050505;
        animation: fadeZoom 0.8s ease-out;
      }

      @keyframes fadeZoom {
  from {
    opacity: 0;
    transform: scale(1.1);
  }
  to {
    opacity: 1;
    transform: scale(1);
  }
}


      .wrapper {
