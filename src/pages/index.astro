     .twotwo .subtitle {
  margin: 20px 15px; /* Mantener tus márgenes existentes */
  
  /* Fondo del texto con un gradiente */
  background: linear-gradient(
    90deg,
    white,           /* Inicia en blanco */
    skyblue,            /* Pasa por gris */
    white,           /* Vuelve a blanco para un ciclo suave */
    skyblue
  );
  background-size: 300%;
  animation: gradientShift 5s linear infinite alternate;
  -webkit-background-clip: text;
  background-clip: text;
  -webkit-text-fill-color: transparent;
  color: transparent; /* Fallback para navegadores que no soportan -webkit-text-fill-color */
  overflow: hidden; /* Oculta cualquier parte del gradiente que se salga del contenedor */
}

/* Definición de la animación */
@keyframes gradientShift {
  0% {
    background-position: 0% center; /* El gradiente empieza a la izquierda */
  }
  100% {
    background-position: 100% center; /* El gradiente se mueve completamente a la derecha */
  }
}
